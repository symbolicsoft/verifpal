attacker[active]

principal Bob[
	knows private k 
]

principal Alice[
	knows password a
	knows private k
	e = ENC(k, a)
	s = SIGN(k, a)
]

Alice -> Bob: e

principal Bob[
	m1 = DEC(k, e)
]

queries[
	confidentiality? a
]
