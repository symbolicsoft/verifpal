attacker[active]

principal Alice[
	knows private x
	knows private k
	e = ENC(k, x)
]

Alice -> Bob: e

principal Bob[
	knows private k
	d = DEC(k, e)
]

queries[
	confidentiality? x
	authentication? Alice -> Bob: e
]
